   "source": [
    "def factorial(n):\n",
    "    if n== 1:\n",
    "        return n\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "\n",
    "number = int(input(\"Enter whose number to be factorialed : \"))\n",
    "if number == 0:\n",
    "    print(\"Factorial is \",1)\n",
    "else:\n",
    "    print(\"Factorial is \",factorial(number))"
   ]
  }
